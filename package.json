{
  "name": "oceanprotocol-site",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://oceanprotocol.com",
  "scripts": {
    "start": "react-scripts start",
    "build": "./scripts/build.sh",
    "lint:js": "eslint ./{src,public}/**/*.js",
    "lint:css": "stylelint ./src/**/*.js",
