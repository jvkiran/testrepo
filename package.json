{
  "name": "@oceanprotocol/site",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://oceanprotocol.com",
  "scripts": {
    "start": "react-scripts start",
    "build": "./scripts/build.sh",
    "ssr": "npm run build && serve -s build",
    "lint": "eslint ./src/**/*.js",
    "format": "prettier ./src/**/*.js --write",
    "test": "npm run lint && react-scripts test",
    "deploy": "./scripts/deploy.sh",
    "eject": "react-scripts eject",
    "postbuild": "react-snap"
  },
  "dependencies": {
    "@oceanprotocol/art": "^1.0.2",
    "axios": "^0.18.0",
    "cookies-eu-banner": "^1.2.14",
    "jsonp": "^0.2.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-animations": "^1.0.0",
    "react-collapsible": "^2.3.1",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-lazyload": "^2.3.0",
    "react-player": "^1.6.6",
    "react-remarkable": "^1.1.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.5",
    "react-visibility-sensor": "^5.0.1",
    "smooth-scroll": "^14.2.1",
    "styled-components": "^4.0.0",
    "three": "^0.97.0"
  },
  "devDependencies": {
    "eslint": "5.6.0",
    "eslint-config-oceanprotocol": "^1.2.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "react-snap": "^1.19.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "reactSnap": {
    "fixWebpackChunksIssue": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
